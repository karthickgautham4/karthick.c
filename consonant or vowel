#include<stdio.h>
int main()
{
char ch;
clrscr();
printf("Enter the character to check Vowel or Consonant");
scanf("%c",&ch);
if(ch=='a',ch=='A',ch=='e',ch=='E',ch=='i',ch=='I',ch=='o',ch=='O',ch=='u',ch=='U')
printf("the character is a Vowel");
else 
printf("the character is a Consonant");
getch();
}
